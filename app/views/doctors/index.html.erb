<h2>登録医師一覧</h2>
<div class="d-flex align-items-center">
    <table class="table table-hover doctors__table">
        <thead class="thead-dark">
            <tr>
                <!-- <th>ID</th> -->
                <th>医師名</th>
                <th>性別</th>
            </tr>
        </thead>
        <tbody>
            <% @doctors.each do |doctor| %></tr>
            <tr>
                <td><%= doctor.doctorname %></td>
                <td><%= doctor.sex %></td>
            </tr>
            <% end %>
        </tbody>
    </table>
</div>